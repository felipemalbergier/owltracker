{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
"configurations": [{
    "name": "Run Main",
    "type": "debugpy",
    "request": "launch",
    "program": "${workspaceFolder}/owltracker/main.py",
    "console": "integratedTerminal",
    "justMyCode": true,
    "python": "/home/felipe/venvs/owl/bin/python",
    "cwd": "${workspaceFolder}/",
    "env": {
        "PYTHONPATH": "${workspaceFolder}/"
    }
    },
    {
    "name": "Python: Current File",
    "type": "debugpy",
    "request": "launch",
    "program": "${file}",
    "console": "integratedTerminal",
    "cwd": "${workspaceFolder}",
    "python": "your/python/path",
    "env": {
        "PYTHONPATH": "${workspaceFolder}/"
    }
    }
    ,
    {
        "name": "Run Unittests",
        "type": "debugpy",
        "request": "launch",
        "module": "unittest",
        "args": [
            "discover",
            "-s",
            "tests"
        ],
        "console": "integratedTerminal",
        "justMyCode": true,
        "python": "your/python/path",
        "cwd": "${workspaceFolder}/",
        "env": {
            "PYTHONPATH": "${workspaceFolder}/"
        }
    }
]}